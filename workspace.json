{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "api-gateway-admin",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "scss", "linter": "eslint", "babel": true },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "api-gateway-admin": {
      "root": "apps/api-gateway-admin",
      "sourceRoot": "apps/api-gateway-admin/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api-gateway-admin",
            "index": "apps/api-gateway-admin/src/index.html",
            "main": "apps/api-gateway-admin/src/main.tsx",
            "polyfills": "apps/api-gateway-admin/src/polyfills.ts",
            "tsConfig": "apps/api-gateway-admin/tsconfig.app.json",
            "assets": [
              "apps/api-gateway-admin/src/favicon.ico",
              "apps/api-gateway-admin/src/assets"
            ],
            "styles": ["apps/api-gateway-admin/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/api-gateway-admin/src/environments/environment.ts",
                  "with": "apps/api-gateway-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "api-gateway-admin:build",
            "proxyConfig": "apps/api-gateway-admin/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "api-gateway-admin:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-gateway-admin/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-gateway-admin"],
          "options": {
            "jestConfig": "apps/api-gateway-admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-gateway-admin-e2e": {
      "root": "apps/api-gateway-admin-e2e",
      "sourceRoot": "apps/api-gateway-admin-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/api-gateway-admin-e2e/cypress.json",
            "tsConfig": "apps/api-gateway-admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "api-gateway-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "api-gateway-admin:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api-gateway-admin-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "api:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/api/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/api-interfaces/**/*.ts"] }
        }
      }
    },
    "node-jwt": {
      "root": "libs/node-jwt",
      "sourceRoot": "libs/node-jwt/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/node-jwt/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/node-jwt"],
          "options": {
            "jestConfig": "libs/node-jwt/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/node-jwt",
            "tsConfig": "libs/node-jwt/tsconfig.lib.json",
            "packageJson": "libs/node-jwt/package.json",
            "main": "libs/node-jwt/src/index.ts",
            "assets": ["libs/node-jwt/*.md"]
          }
        }
      }
    },
    "config": {
      "root": "libs/config",
      "sourceRoot": "libs/config/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/config/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/config"],
          "options": {
            "jestConfig": "libs/config/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
