{
   "compilerOptions": {
      "jsx": "react",
      "allowJs": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "es6",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "lib": ["es2015", "dom", "ES2016"],
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
         "*": [
            "src/*"
         ]
      }
   },
   "exclude": [
      "node_modules",
   ]
}